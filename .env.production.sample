# Database connection used by the application
SUPABASE_DATABASE_URL=postgres://postgres:postgres@db:5432/tinylink
DB_SCHEMA=tinylink

# Frontend Supabase auth configuration (client-side)
# These values are now injected at runtime through the /runtime-config.js endpoint
# When running in Docker, make sure these environment variables are passed to the container
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Application settings
NODE_ENV=production