# Supabase connection string (provided by Supabase)
SUPABASE_DATABASE_URL=postgres://postgres:[password]@[your-project].supabase.co:5432/postgres
DB_SCHEMA=tinylink

# Frontend Supabase auth configuration (client-side)
VITE_SUPABASE_URL=https://[your-project].supabase.co
VITE_SUPABASE_ANON_KEY=[your-supabase-anon-key]

# Application settings
NODE_ENV=development